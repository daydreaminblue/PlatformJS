<link rel="stylesheet" href="/resources/styles/list.css" />

<div class="left_side">
  <div class="table" id="myTable">
    <div class="table__options">
      <p>نام کاربر</p>
      <p>عنوان دوره</p>
      <p>قیمت</p>
    </div>

    <div class="table__no__wrapper">
      <% orders.forEach(order => { %>

      <div class="table__no">
        <p><%= order.user.name %></p>
        <p><%= order.course.title %></p>
        <p><%= order.course.price %></p>
      </div>

      <% }) %>
    </div>

    <%- include(viewPath('layouts/pagination') ,{ pagination : orders}) %>
  </div>
</div>
